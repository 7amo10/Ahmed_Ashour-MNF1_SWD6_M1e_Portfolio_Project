{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": "off",
    "axe/aria": "off",
    "axe/text-alternatives": "off",
    "no-inline-styles": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[loading]"
        ]
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "overflow: clip",
          "-webkit-overflow-scrolling"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not firefox <= 127",
    "not safari <= 17.5"
  ]
}